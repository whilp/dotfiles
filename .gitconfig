[alias]
	pr = pull-request
	ph = push -q
	pl = pull -q
	st = status -sb --porcelain
	ci = commit
	ca = commit -a
	ce = "!git add -e . && git commit -v"
	glog = log --oneline --decorate --graph
	tags = tag -l
	man = ls-files
	bare = config --bool core.bare true
	review = merge --no-ff --no-commit
	ctags = "!sh -c '[ -f .git/hooks/ctags ] || git init; .git/hooks/ctags' git-ctags"
[core]
	editor = Open
	excludesfile = ~/.gitconf/ignore
	pager = nobs
[format]
	pretty = format:"%h %p %d %ai %ce%n  %s%n"
[url "git@github.com:"]
	insteadOf = gh:
	pushInsteadOf = https://github.com/
[url "git@github.com:"]
	insteadOf = https://github.com/
[url "git@gist.github.com:"]
	insteadOf = gist:
	insteadOf = https://gist.github.com/
[url "git@github.banksimple.com:"]
	insteadOf = bh:
	insteadOf = https://github.banksimple.com/
	insteadOf = git://github.banksimple.com/
	pushInsteadOf = https://github.banksimple.com/
[push]
	default = current
[pull]
	fast-forward = true
	rebase = true
[init]
	templatedir = ~/.gitconf/init
[remote "origin"]
	fetch = +refs/pull/*/head:refs/pull/origin/*
[hub]
	host = github.banksimple.com
[merge]
	tool = emerge
	conflictstyle = diff3
[github]
	user = whilp
[gh]
	host = github.banksimple.com
	autoUpdate = never
[remote]
	pushDefault = origin
[user]
	email = wcmaier@gmail.com
	name = Will Maier
[man]
	viewer = nobs
[man "nobs"]
	cmd = man -P nobs
[color]
	ui = false
