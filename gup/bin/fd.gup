#!/bin/sh

set -eux

sum="95218c2d567a2f84a56853f09a225421321346a027f21f5e9b969c4ea7b0c58a"
url="https://github.com/sharkdp/fd/releases/download/v6.1.0/fd-v6.1.0-x86_64-unknown-linux-musl.tar.gz"

tmp=$(mktemp -d)
trap "rm -rf $tmp" EXIT
f="$tmp/tgz"
get-and-check "$url" "$f" "$sum"
tar -xzf "$f" --strip-components=1 -C "$tmp"
mv "$tmp/fd" "$1"

