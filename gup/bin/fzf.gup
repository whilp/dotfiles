#!/bin/sh

set -eux

sum=1e02cf9a2d5cb1343f5fb2f47d4c71ccc5a1750f2a9a1921fbd27a1dd75b851b
url=https://github.com/junegunn/fzf-bin/releases/download/0.17.3/fzf-0.17.3-linux_amd64.tgz

tmp=$(mktemp -d)
trap "rm -rf" EXIT
f="$tmp/fzf"
get-and-check "$url" "$f" "$sum"
tar -xzf "$f" fzf -O > "$1"
chmod a+x "$1"